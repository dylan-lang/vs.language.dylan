{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "stopOnEntry": false,
      "sourceMaps": true,
      "outFiles": [
        "out/src/dylanMain.ts1"
      ],
      "preLaunchTask": "npm: compile"
    },
    {
      "name": "Launch Tests",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--extensionTestsPath=${workspaceFolder}/out/test"
      ],
      "stopOnEntry": false,
      "sourceMaps": true,
      "outFiles": [
        "out/test/dylanMain.test.ts"
      ],
      "preLaunchTask": "npm"
    }
  ]
}